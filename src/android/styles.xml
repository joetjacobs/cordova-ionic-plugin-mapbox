<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#33C377</item>
        <item name="colorPrimaryDark">#FF218450</item>
        <item name="colorAccent">#FF4081</item>
    </style>

    <style name="NavigationMapRoute">
        <!-- Colors -->
        <item name="routeColor">#4264fb</item>
        <item name="routeModerateCongestionColor">#d9d838</item>
        <item name="routeSevereCongestionColor">#ee4e8b</item>
        <item name="routeShieldColor">#ffffff</item>
        <!-- Scales -->
        <item name="routeScale">1.0</item>
    </style>

    <style name="NavigationView" parent="Theme.AppCompat.NoActionBar">
        <item name="navigationViewPrimary">#ffffff</item>
        <item name="navigationViewSecondary">#4264fb</item>
        <item name="navigationViewAccent">#ee4e8b</item>
    </style>

</resources>